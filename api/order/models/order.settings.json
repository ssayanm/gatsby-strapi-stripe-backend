{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "products": {
      "collection": "product",
      "via": "orders",
      "dominant": true
    },
    "number": {
      "type": "string"
    },
    "total": {
      "type": "float"
    },
    "subtotal": {
      "type": "decimal"
    },
    "taxes": {
      "type": "float"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "product_qty": {
      "type": "json"
    }
  }
}
